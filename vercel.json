{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://moviesappbackend.onrender.com/api/$1"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://moviesappbackend.onrender.com/api/$1",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
